import React, { useState } from "react";
import { Pagination } from "../../../client/src/components/ui/pagination";

export default function PromptTemplateListStoryboard() {
  const [currentPage, setCurrentPage] = useState(1);
  const totalPages = 5;

  return (
    <div className="bg-white p-6 rounded-md shadow-md">
      <h1 className="text-2xl font-bold mb-4">Prompt Templates</h1>
      <div className="grid gap-4 mb-6">
        {[1, 2, 3, 4].map((item) => (
          <div key={item} className="border p-4 rounded-md">
            <h2 className="font-semibold">Template {item}</h2>
            <p className="text-gray-600 text-sm">Sample template description</p>
          </div>
        ))}
      </div>
      <Pagination 
        currentPage={currentPage} 
        totalPages={totalPages} 
        onPageChange={setCurrentPage} 
      />
    </div>
  );
}
